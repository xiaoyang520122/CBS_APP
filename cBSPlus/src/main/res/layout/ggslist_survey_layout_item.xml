<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="5dp"
        android:paddingTop="10dp"
        android:paddingRight="5dp"
        android:paddingBottom="5dp" >

        <TextView
            android:id="@+id/ggslsli_nameAndDept"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="姓名 机构名称"
            android:textColor="@color/hui_text_xxxxh"
            android:textSize="@dimen/text_15" />

        <TextView
            android:id="@+id/ggslsli_zxBan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:background="@drawable/corners_yellow_bg_hui_line_5dp"
            android:paddingBottom="2dp"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:paddingTop="2dp"
            android:text="主"
            android:textColor="@color/white"
            android:textSize="@dimen/text_l" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawablePadding="3dp"
            android:gravity="right"
            android:text=""
            android:textColor="@color/bule_text_h"
            android:textSize="@dimen/order_list_defaul" />

        <TextView
            android:id="@+id/ggslsli_diaoduTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/time_s_bule"
            android:drawablePadding="3dp"
            android:text="0000-00-00"
            android:textColor="@color/bule_text_h"
            android:textSize="@dimen/order_list_defaul" />

        <TextView
            android:id="@+id/ggslsli_diaoduStatus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:background="@drawable/corners_hui_line5"
            android:paddingBottom="1dp"
            android:paddingLeft="3dp"
            android:paddingRight="3dp"
            android:paddingTop="1dp"
            android:text="未调度"
            android:textColor="@color/hui_text_xh"
            android:textSize="@dimen/text_s" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:paddingBottom="10dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="联系电话："
            android:textColor="@color/hui_text_xh"
            android:textSize="@dimen/text_l" />
        <TextView
            android:id="@+id/ggslsli_ggsphoneNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="无"
            android:layout_marginRight="15dp"
            android:textColor="@color/hui_text_xh"
            android:textSize="@dimen/text_l" />
        
         <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="贡献比例："
            android:textColor="@color/hui_text_xh"
            android:textSize="@dimen/text_l" />
        <TextView
            android:id="@+id/ggslsli_gxbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0.0"
            android:textColor="@color/hui_text_xh"
            android:textSize="@dimen/text_l" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="%"
            android:textColor="@color/hui_text_xh"
            android:textSize="@dimen/order_list_defaul" />
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:drawablePadding="3dp"
            android:gravity="right"
            android:text=""
            android:textColor="@color/bule_text_h"
            android:textSize="@dimen/order_list_defaul" />
        <TextView
            android:id="@+id/ggslsli_deleteGgs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:background="@drawable/corners_hui_line5"
            android:paddingBottom="1dp"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingTop="1dp"
            android:text="删除"
            android:textColor="@color/yellow_baner"
            android:textSize="@dimen/text_l" />
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawablePadding="3dp"
            android:gravity="right"
            android:text=""
            android:textColor="@color/bule_text_h"
            android:textSize="@dimen/order_list_defaul" />
    </LinearLayout>
    <View
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:background="@color/hui_bg" />

</LinearLayout>