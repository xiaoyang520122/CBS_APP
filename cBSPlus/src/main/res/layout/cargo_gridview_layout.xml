<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/cargo_gridView_wsline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible"
        android:background="@drawable/white_bg_bule_line"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/cargo_gridView_wstitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:ellipsize="marquee"
            android:gravity="right"
            android:marqueeRepeatLimit="marquee_forever"
            android:text="损失物品类"
            android:textColor="@color/bulue_main"
            android:textSize="@dimen/text_15"
            android:textStyle="bold" />

    </LinearLayout>

    <com.cninsure.cp.view.MyGridView
        android:id="@+id/cargo_gridView_gridView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:horizontalSpacing="2dp"
        android:listSelector="@android:color/transparent"
        android:numColumns="4"
        android:verticalSpacing="10dp"></com.cninsure.cp.view.MyGridView>


</LinearLayout>
