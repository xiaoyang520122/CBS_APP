<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="5dp" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal" >
        <TextView
            android:id="@+id/FSXDSPList_createDate"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:layout_marginLeft="5dp"
            android:drawableLeft="@drawable/check_date_hui32"
            android:drawablePadding="5dp"
            android:gravity="left"
            android:padding="2dp"
            android:text="调度时间"
            android:textColor="@color/hui_text_xh"
            android:textSize="@dimen/order_list_defaul" />
        <TextView
            android:id="@+id/FSXDSPList_UseTime"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            android:drawableLeft="@drawable/yuyue_time_yellow32"
            android:drawablePadding="5dp"
            android:gravity="left"
            android:padding="2dp"
            android:text="用时："
            android:visibility="gone"
            android:textColor="@color/hui_text_xh"
            android:textSize="@dimen/order_list_defaul" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/corners_white2_4_10dp"
        android:orientation="vertical"
        android:paddingBottom="5dp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:paddingTop="10dp" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="3dp"
            android:gravity="center_horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/FSXDSPListI_product"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:ellipsize="marquee"
                        android:gravity="center"
                        android:hint="产品细类"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:paddingTop="1dp"
                        android:singleLine="true"
                        android:textColor="@color/hui_text_xxxxh"
                        android:textStyle="bold"
                        android:textSize="@dimen/activity_vertical_margin" />

                    <com.cninsure.cp.view.MarqueeTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:hint=" | "
                        android:textColor="@color/hui_text_xh"
                        android:textSize="@dimen/text_m" />

                    <com.cninsure.cp.view.MarqueeTextView
                        android:id="@+id/FSXDSPListI_BussType"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/corners_read_4_10dp"
                        android:ellipsize="marquee"
                        android:gravity="center"
                        android:hint="业务品种"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:paddingBottom="1dp"
                        android:paddingLeft="5dp"
                        android:paddingRight="5dp"
                        android:paddingTop="1dp"
                        android:singleLine="true"
                        android:textColorHint="@color/white"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_defaul" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="right">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:layout_marginRight="5dp"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/FSXDSPList_bussType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:paddingBottom="1dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:paddingTop="1dp"
                    android:text="佣金"
                    android:textColor="@color/hui_text_h"
                    android:textSize="@dimen/text_l" />

                <TextView
                    android:id="@+id/FSXDSPListI_surveyorFee"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:gravity="center"
                    android:paddingBottom="1dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:paddingTop="1dp"
                    android:text="0.0"
                    android:textStyle="bold"
                    android:textColor="@color/yellow_logo"
                    android:textSize="@dimen/text_defaul" />
            </LinearLayout>

        </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:background="@color/hui_line" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/FSXDSPListI_ggsName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:ellipsize="marquee"
                android:gravity="center_vertical|left"
                android:drawableRight="@drawable/callphone_blue_40"
                android:marqueeRepeatLimit="marquee_forever"
                android:singleLine="true"
                android:text="案件对接人"
                android:textColor="@color/hui_text_xxxxh"
                android:textSize="@dimen/activity_vertical_margin" />

            <com.cninsure.cp.view.MarqueeTextView
                android:id="@+id/FSXDSPListI_workOrg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:ellipsize="marquee"
                android:gravity="center"
                android:marqueeRepeatLimit="marquee_forever"
                android:paddingLeft="3dp"
                android:paddingTop="1dp"
                android:singleLine="true"
                android:text="作业归属机构！"
                android:textColor="@color/hui_text_xh"
                android:textSize="@dimen/text_l" />
            <TextView
                android:id="@+id/FSXDSPListI_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ellipsize="marquee"
                android:gravity="right"
                android:marqueeRepeatLimit="marquee_forever"
                android:text="任务状态"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:textColor="@color/bulue_main"
                android:textSize="@dimen/text_sl"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/FSXDSPListI_wtTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:gravity="center_vertical|left"
                android:paddingBottom="1dp"
                android:paddingTop="1dp"
                android:text="委托人："
                android:textColor="@color/hui_text_h"
                android:textSize="@dimen/order_list_defaul" />

            <com.cninsure.cp.view.MarqueeTextView
                android:id="@+id/FSXDSPListI_wtName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:ellipsize="marquee"
                android:gravity="center"
                android:hint="无委托人信息！"
                android:marqueeRepeatLimit="marquee_forever"
                android:paddingLeft="3dp"
                android:paddingTop="1dp"
                android:singleLine="true"
                android:textColor="@color/hui_text_xh"
                android:textColorHint="@color/hui_text_h"
                android:textSize="@dimen/text_sl" />

            <TextView
                android:id="@+id/FSXDSPListI_copy_wtName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:drawableLeft="@drawable/copy_hui_36"
                android:paddingLeft="3dp"
                android:paddingTop="1dp"
                android:paddingRight="3dp"
                android:paddingBottom="1dp"
                android:textColor="@color/hui_text_xh"
                android:textSize="@dimen/text_l" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:gravity="center_vertical|left"
                android:paddingBottom="1dp"
                android:paddingTop="1dp"
                android:text="任务地点："
                android:textColor="@color/hui_text_xh"
                android:textSize="@dimen/order_list_defaul" />

            <com.cninsure.cp.view.MarqueeTextView
                android:id="@+id/FSXDSPListI_taskAddress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:ellipsize="marquee"
                android:gravity="center"
                android:hint="无任务地址！"
                android:drawableRight="@drawable/copy_hui_36"
                android:marqueeRepeatLimit="marquee_forever"
                android:paddingLeft="3dp"
                android:paddingTop="1dp"
                android:singleLine="true"
                android:textColor="@color/hui_text_xxxxh"
                android:textSize="@dimen/text_m" />

            <TextView
                android:id="@+id/FSXDSPListI_copy_taskAddress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:drawableRight="@drawable/route_blue_40"
                android:paddingBottom="1dp"
                android:paddingLeft="3dp"
                android:paddingRight="3dp"
                android:paddingTop="1dp"
                android:textColor="@color/hui_text_xh"
                android:textSize="@dimen/text_l" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:gravity="center_vertical|left"
                android:paddingBottom="1dp"
                android:paddingTop="1dp"
                android:text="受损基本情况："
                android:textColor="@color/hui_text_xh"
                android:textSize="@dimen/order_list_defaul" />

            <TextView
                android:id="@+id/FSXDSPListI_damagedState"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:ellipsize="marquee"
                android:gravity="center"
                android:hint="无信息！"
                android:marqueeRepeatLimit="marquee_forever"
                android:paddingLeft="3dp"
                android:paddingTop="1dp"
                android:singleLine="true"
                android:drawableRight="@drawable/msg_hui_36"
                android:textColor="@color/hui_text_xxxxh"
                android:textSize="@dimen/text_m" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:gravity="center_vertical|left"
                android:paddingBottom="1dp"
                android:paddingTop="1dp"
                android:text="查勘重点要求："
                android:textColor="@color/hui_text_xh"
                android:textSize="@dimen/order_list_defaul" />

            <TextView
                android:id="@+id/FSXDSPListI_keyPoint"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:ellipsize="marquee"
                android:gravity="center"
                android:hint="无信息！"
                android:marqueeRepeatLimit="marquee_forever"
                android:paddingLeft="3dp"
                android:paddingTop="1dp"
                android:singleLine="true"
                android:drawableRight="@drawable/msg_hui_36"
                android:textColor="@color/hui_text_xxxxh"
                android:textSize="@dimen/text_m" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:gravity="center_vertical|left"
                android:paddingBottom="1dp"
                android:paddingTop="1dp"
                android:text="编号："
                android:textColor="@color/hui_text_xh"
                android:textSize="@dimen/order_list_defaul" />

            <com.cninsure.cp.view.MarqueeTextView
                android:id="@+id/FSXDSPListI_uid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:ellipsize="marquee"
                android:gravity="center"
                android:hint="无任务编号！"
                android:marqueeRepeatLimit="marquee_forever"
                android:paddingLeft="3dp"
                android:paddingTop="1dp"
                android:singleLine="true"
                android:textColor="@color/hui_text_xh"
                android:textColorHint="@color/hui_text_h"
                android:textSize="@dimen/text_sl" />

            <TextView
                android:id="@+id/FSXDSPListI_copy_uid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:drawableLeft="@drawable/copy_hui_36"
                android:paddingBottom="1dp"
                android:paddingLeft="3dp"
                android:paddingRight="3dp"
                android:paddingTop="1dp"
                android:textColor="@color/hui_text_xh"
                android:textSize="@dimen/text_l" />
        </LinearLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:background="@color/white" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="2dp" >

            <TextView
                android:id="@+id/FSXDSPListI_button1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="5dp"
                android:text="拒绝任务"
                android:textColor="@color/yellow_logo"
                android:textSize="@dimen/order_list_defaul" />
            <!-- android:background="@drawable/corners_hui_line5" -->

            <TextView
                android:id="@+id/FSXDSPListI_button_line"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical|left"
                android:text=" | "
                android:textColor="@color/hui_text_xh" />

            <TextView
                android:id="@+id/FSXDSPListI_button2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="5dp"
                android:text="接受任务"
                android:textColor="@color/bulue_main"
                android:textSize="@dimen/order_list_defaul" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>