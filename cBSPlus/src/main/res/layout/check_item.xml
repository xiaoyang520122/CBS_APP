<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/video_preview_item"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="5dp"
    android:gravity="center_vertical"
    android:background="@drawable/corners_hui_bg_4_3dp">
    <TextView
        android:id="@+id/video_preview_item_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_defaul"
        android:text="测试选项"
        android:textColor="@color/hui_text_xxxxh"/>
    <CheckBox
        android:id="@+id/video_previ"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/CustomCheckboxTheme" />
</LinearLayout>
