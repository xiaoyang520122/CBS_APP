<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/hui_bg"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/corners_white2_4_10dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/ONLI_title_linear"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingLeft="5dp"
            android:paddingTop="10dp"
            android:paddingRight="5dp"
            android:paddingBottom="10dp">

            <TextView
                android:id="@+id/ONLI_time"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableLeft="@drawable/time_s_bule"
                android:drawablePadding="3dp"
                android:gravity="center_vertical|left"
                android:text="@string/diaodu_time"
                android:textColor="@color/bule_text_h"
                android:textSize="@dimen/text_defaul" />

            <TextView
                android:id="@+id/ONLI_CaseType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical|left"
                android:text=""
                android:textColor="@color/bule_text_h" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical|left"
                android:text=" | "
                android:textColor="@color/hui_text_xh" />

            <TextView
                android:id="@+id/ONLI_bussType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical|left"
                android:text=""
                android:textColor="@color/bule_text_h" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="10dp"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/ONLI_timeOut"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/corners_bule_30dp"
                android:paddingLeft="5dp"
                android:paddingTop="2dp"
                android:paddingRight="10dp"
                android:paddingBottom="2dp"
                android:text="剩余时间："
                android:textColor="@color/bule_text_h"
                android:textSize="@dimen/order_list_defaul" />

            <TextView
                android:id="@+id/ONLI_bohuiInfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="20dp"
                android:drawableLeft="@drawable/c_35"
                android:gravity="center"
                android:paddingLeft="3dp"
                android:paddingTop="1dp"
                android:text="@string/bh_info"
                android:textColor="@color/bule_text_h"
                android:textSize="@dimen/text_m"
                android:visibility="gone" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingRight="10dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="5dp"
                    android:gravity="center_vertical|left"
                    android:paddingTop="1dp"
                    android:text="报案号"
                    android:textColor="@color/hui_text_xh"
                    android:textSize="@dimen/order_list_defaul" />

                <TextView
                    android:id="@+id/ONLI_mcORcNo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="5dp"
                    android:gravity="center_vertical|left"
                    android:text="案件名称"
                    android:textColor="@color/hui_text_xh"
                    android:textSize="@dimen/order_list_defaul" />

                <TextView
                    android:id="@+id/ONLI_mcWtren"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="5dp"
                    android:gravity="center_vertical|left"
                    android:text="委托人"
                    android:textColor="@color/hui_text_xh"
                    android:textSize="@dimen/order_list_defaul" />

                <TextView
                    android:id="@+id/ONLI_mcContect"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="5dp"
                    android:gravity="center_vertical|left"
                    android:text="联系人"
                    android:textColor="@color/hui_text_xh"
                    android:textSize="@dimen/order_list_defaul" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="6dp"
                    android:gravity="center_vertical|left"
                    android:text="案件状态"
                    android:textColor="@color/hui_text_xh"
                    android:textSize="@dimen/order_list_defaul" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="2dp"
                    android:gravity="center_vertical|left"
                    android:text="地址"
                    android:textColor="@color/hui_text_xh"
                    android:textSize="@dimen/order_list_defaul" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginBottom="5dp"
                    android:orientation="horizontal">


                    <TextView
                        android:id="@+id/ONLI_baoanno"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:hint="无"
                        android:textColor="@color/hui_text_xxxxh"
                        android:textSize="@dimen/order_list_defaul" />

                    <TextView
                        android:id="@+id/ONLI_copy_baoan_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/corners_hui_line5"
                        android:drawableLeft="@drawable/copy_somshing_yellow35"
                        android:paddingLeft="3dp"
                        android:paddingTop="1dp"
                        android:paddingRight="3dp"
                        android:text="@string/copytext"
                        android:textColor="@color/hui_text_xh"
                        android:textSize="@dimen/order_list_defaul" />

                    <TextView
                        android:id="@+id/ONLI_WT_info"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:drawableLeft="@drawable/wt_info_blue35"
                        android:gravity="center"
                        android:paddingLeft="3dp"
                        android:paddingTop="1dp"
                        android:text="@string/WT_info"
                        android:textColor="@color/hui_text_xh"
                        android:textSize="@dimen/text_m" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_marginBottom="5dp"
                    android:orientation="horizontal">

                    <com.cninsure.cp.view.MarqueeTextView
                        android:id="@+id/ONLI_mcORcNoTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:ellipsize="marquee"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:singleLine="true"
                        android:text="无"
                        android:textColor="@color/hui_text_xxxxh"
                        android:textSize="@dimen/order_list_defaul" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ONLI_mcWtrenLO"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_marginBottom="5dp"
                    android:orientation="horizontal">

                    <com.cninsure.cp.view.MarqueeTextView
                        android:id="@+id/ONLI_wtren"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:ellipsize="marquee"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:singleLine="true"
                        android:text="无"
                        android:textColor="@color/hui_text_xxxxh"
                        android:textSize="@dimen/order_list_defaul" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ONLI_mcContectLO"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:orientation="horizontal">

                    <com.cninsure.cp.view.MarqueeTextView
                        android:id="@+id/ONLI_cxUintLink"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:text="无姓名"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:textColor="@color/hui_text_xxxxh"
                        android:textSize="@dimen/order_list_defaul" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:text="/"
                        android:textColor="@color/hui_text_xh"
                        android:textSize="@dimen/order_list_defaul" />

                    <TextView
                        android:id="@+id/ONLI_lxPhone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:drawablePadding="5dp"
                        android:gravity="center_vertical|right"
                        android:text="无号码"
                        android:textColor="@color/hui_text_xxxxh"
                        android:textSize="@dimen/order_list_defaul" />

                    <TextView
                        android:id="@+id/ONLI_lxPhone_call"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/corners_hui_line5"
                        android:drawableLeft="@drawable/serve_tel_yellow35"
                        android:paddingLeft="3dp"
                        android:paddingTop="1dp"
                        android:paddingRight="3dp"
                        android:text="@string/callPhone"
                        android:textColor="@color/hui_text_xh"
                        android:textSize="@dimen/text_l" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_marginBottom="5dp"
                    android:orientation="horizontal">
                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"/>

                    <com.cninsure.cp.view.MarqueeTextView
                        android:id="@+id/ONLI_status"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/corners_hui_line5"
                        android:ellipsize="marquee"
                        android:gravity="center_vertical|left"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:paddingLeft="3dp"
                        android:paddingTop="1dp"
                        android:paddingBottom="1dp"
                        android:paddingRight="3dp"
                        android:singleLine="true"
                        android:text="未知状态"
                        android:textColor="@color/yellow_baner"
                        android:textSize="@dimen/text_l" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.cninsure.cp.view.MarqueeTextView
                        android:id="@+id/ONLI_address"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:ellipsize="marquee"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:singleLine="true"
                        android:text="无"
                        android:textColor="@color/hui_text_xxxxh"
                        android:textSize="@dimen/order_list_defaul" />


                    <TextView
                        android:id="@+id/ONLI_naviGation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginLeft="5dp"
                        android:drawableLeft="@drawable/navigati_blue35"
                        android:gravity="center"
                        android:paddingLeft="3dp"
                        android:paddingRight="3dp"
                        android:textColor="@color/hui_text_xh"
                        android:textSize="@dimen/text_m" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="10dp">

            <TextView
                android:id="@+id/ONLI_cancel_order"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="5dp"
                android:text="取消订单"
                android:textColor="@color/hui_text_xh"
                android:textSize="@dimen/order_list_defaul" />
            <!-- android:background="@drawable/corners_hui_line5" -->

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical|left"
                android:text=" | "
                android:textColor="@color/hui_text_xh" />

            <TextView
                android:id="@+id/ONLI_accept_order"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="5dp"
                android:text="接受订单"
                android:textColor="@color/bulue_main"
                android:textSize="@dimen/order_list_defaul" />
        </LinearLayout>

        <!--
        LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:visibility="gone"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/ONLI_car"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:layout_marginLeft="22dp"
                android:layout_weight="1"
                android:gravity="center_vertical|left"
                android:text="归属机构："
                android:textColor="@color/hui_text_xh"
                android:textSize="@dimen/order_list_defaul" />

            <TextView
                android:id="@+id/ONLI_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:layout_marginLeft="22dp"
                android:layout_weight="1"
                android:gravity="center_vertical|left"
                android:text=""
                android:textColor="@color/bule_text_m"
                android:textSize="@dimen/order_list_defaul" />
        </LinearLayout
        -->
    </LinearLayout>

</LinearLayout>